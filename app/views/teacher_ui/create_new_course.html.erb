<h1>Create New Course</h1>
<%= form_tag(:action => 'create_new_course_control') do %>
    <p>
      Course No : <%= text_field_tag(:no) %>
    </p>

    <p>
      Course Name : <%= text_field_tag(:name) %>
    </p>

    <p>
      Explain : <%= text_field_tag(:explain) %>
    </p>

    <%= submit_tag("Add") %>
<% end %>