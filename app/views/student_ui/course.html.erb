<%= link_to :'Home', :controller => :student_ui,  :action => :index %><br>
<%= link_to :'My Profile', :controller => :student_ui,  :action => :my_profile %><br>
<%= link_to :'Edit Profile', :controller => :student_ui,  :action => :edit_profile %><br>
<%= link_to :'All Courses', :controller => :student_ui,  :action => :all_courses %><br>
<%= link_to :'My Courses', :controller => :student_ui,  :action => :my_courses %><br>
<%= link_to :'Grades', :controller => :student_ui,  :action => :grades, :course_id => @course.id %><br>

<%= link_to :'Logout', :controller => :sessions,  :action => :logout %><br>

<p>
  Course No : <%= @course.no %>
</p>
<p>
    Course Name : <%= @course.name %>
</p>
<p>
    Course Explain : <%= @course.explain %>
</p>

<h1>Lectures</h1>

<% @lectures.each do |lecture| %>
    <p>
        <%= lecture.name %>
    </p>
<% end %>

<h1>Projects</h1>

<% @projects.each do |project| %>
    <p>
        Project Name : <%= project.name %>
    </p>

    <p>
        Project Explain : <%= project.explain %>
    </p>

    <% @files.each do |file| %>
        <% if file.project_no == project.id.to_s %>
            File Name : <%= file.name %><br>
        <% end %>
    <% end %>
<% end %>


