<%= link_to :Login , :login, class: "nav_links"%>
<%= link_to :Signup , :signup, class: "nav_links"%>

<h1>Sign Up</h1>
<%= form_tag(:action => 'signup_control') do %>
    <p>
      Profil Image : <%= file_field_tag(:profil_image) %>
    </p>

    <p>
      Student Number: <%= text_field_tag(:student_number) %>
    </p>

    <p>
      Name: <%= text_field_tag(:name) %>
    </p>

    <p>
      Password: <%= password_field_tag(:password) %>
    </p>

    <p>
      E-mail: <%= text_field_tag(:email) %>
    </p>

    <%= submit_tag("Sign Up") %>
<% end %>
