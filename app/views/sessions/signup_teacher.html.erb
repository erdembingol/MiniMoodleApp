<%= link_to :Login , :login_teacher, class: "nav_links"%>
<%= link_to :Signup , :signup_teacher, class: "nav_links"%>

<h1>Sign Up</h1>
<%= form_tag(:action => 'signup_teacher_control') do %>
    <p>
      Profil Image : <%= file_field_tag(:profil_image) %>
    </p>

    <p>
      E-mail: <%= text_field_tag(:email) %>
    </p>

    <p>
      Name: <%= text_field_tag(:name) %>
    </p>

    <p>
      Password: <%= password_field_tag(:password) %>
    </p>

    <%= submit_tag("Sign Up") %>
<% end %>